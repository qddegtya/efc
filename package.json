{
  "name": "xaefc",
  "version": "1.0.3",
  "description": "Elegant devtool for formily component.",
  "main": "index.js",
  "bin": {
    "efc": "./bin/efc"
  },
  "files": [
    "lib",
    "shared"
  ],
  "scripts": {
    "cf-add": "./node_modules/.bin/cf add --output=./src/commands --template=./src/commands/cmd.tpl",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint --ext .js src/** --fix",
    "dev": "rimraf ./lib && ./node_modules/.bin/babel src/commands --out-dir lib/commands --watch",
    "build": "rimraf ./lib && ./node_modules/.bin/babel src/commands --out-dir lib/commands"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qddegtya/efc.git"
  },
  "keywords": [
    "formily",
    "vue",
    "react",
    "ts"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=8.10"
  },
  "author": "xiaoa",
  "license": "MIT",
  "devDependencies": {
    "@atools/cf": "^1.0.10",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0"
  },
  "dependencies": {
    "css-loader": "^6.2.0",
    "express": "^4.17.1",
    "inquirer": "^7.3.3",
    "map-stream": "^0.0.7",
    "open": "^8.2.1",
    "sass": "^1.38.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "vinyl-fs": "^3.0.3",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.51.1",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "xajs": "^1.0.13",
    "rimraf": "^3.0.2"
  }
}
